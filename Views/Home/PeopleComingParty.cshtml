@model IEnumerable<GuestResponse>
@{
    ViewData["Title"] = "PeopleComingParty";
}

<h1>People Coming Party</h1>

<div class="mb-3">
    <a asp-action="GuestList">See all answers</a>
</div>
<table class="table">
    <thead>
        <tr>
            <th>Name</th>
        
        </tr>
    </thead>
    <tbody>
        @foreach (var guest in Model)
        {
            <tr>
                <td>@guest.Name</td>
            </tr>
        }
    </tbody>
</table>

